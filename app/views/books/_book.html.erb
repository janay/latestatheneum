<tr>
  <td class="book">
    <span class="title"><%= book.title %></span>
    <span class="timestamp">
      By <%= book.authors %> .
    </span>
  </td>
    <% if current_user?(book.user) %>
  		<td>
    		<%= link_to "delete", book, :method => :delete,
                                     :confirm => "You sure?",
                                     :title => book.title %>
  		</td>
  	<% end %>
</tr>